<Window x:Class="Shootapp.WindowPrint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shootapp"
        mc:Ignorable="d"
        Title="Print Scores" Width="800" Height="1000" Loaded="Window_Loaded">
    <DockPanel>
        <TabControl x:Name="tabRoot" Background="Gray">

            <TabItem Header="TeamDOC">
                <FixedDocument x:Name="fixed_document" />
            </TabItem>

            <TabItem Header="SingleDOC">
                <FixedDocument x:Name="fixed_doc_single" />
            </TabItem>

        </TabControl>
    </DockPanel>
</Window>
